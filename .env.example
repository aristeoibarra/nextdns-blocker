# NextDNS Blocker Configuration
# Copy this file to .env and fill in your values

# NextDNS API credentials (required)
NEXTDNS_API_KEY=your_api_key_here
NEXTDNS_PROFILE_ID=your_profile_id_here

# Timezone for schedule evaluation
TIMEZONE=UTC

# API configuration (optional)
API_TIMEOUT=10
API_RETRIES=3

# Remote domains.json URL (optional)
# DOMAINS_URL=https://example.com/domains.json

# Discord webhook notifications (optional)
# Set DISCORD_NOTIFICATIONS_ENABLED=true to enable notifications
# DISCORD_NOTIFICATIONS_ENABLED=false
# DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/xxx/yyy
